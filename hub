local function C_3()
    local script = G2L["3"]
    local UserInputService = game:GetService("UserInputService")
    local gui = script.Parent
    local dragging
    local dragInput
    local dragStart
    local startPos
    local function update(input)
        local delta = input.Position - dragStart
        gui.Position =
            UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
    gui.InputBegan:Connect(
        function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                dragging = true
                dragStart = input.Position
                startPos = gui.Position
                input.Changed:Connect(
                    function()
                        if input.UserInputState == Enum.UserInputState.End then
                            dragging = false
                        end
                    end
                )
            end
        end
    )
    gui.InputChanged:Connect(
        function(input)
            if
                input.UserInputType == Enum.UserInputType.MouseMovement or
                    input.UserInputType == Enum.UserInputType.Touch
             then
                dragInput = input
            end
        end
    )
    UserInputService.InputChanged:Connect(
        function(input)
            if input == dragInput and dragging then
                update(input)
            end
        end
    )
end
task.spawn(C_3)
local function C_a()
    local script = G2L["a"]
    local Players = game:GetService("Players")
    local LocalPlayer = Players.LocalPlayer
    local TextChatService = game:GetService("TextChatService")
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local StarterGui = game:GetService("StarterGui")
    local textButton = script.Parent
    local textBox = script.Parent.Parent.TextBox
    local chat = TextChatService.ChatInputBarConfiguration.TargetTextChannel
    local function replace(str, find_str, replace_str)
        local escaped_find_str = find_str:gsub("[%-%^%$%(%)%%%.%[%]%*%+%-%?]", "%%%0")
        return str:gsub(escaped_find_str, replace_str)
    end
local letters = {
    ["A"] = "α",
    ["a"] = "α",
    ["B"] = "Ⴆ",
    ["b"] = "Ⴆ",
    ["C"] = "ƈ",
    ["c"] = "ƈ",
    ["D"] = "ԃ",
    ["d"] = "ԃ",
    ["E"] = "ҽ",
    ["e"] = "ҽ",
    ["F"] = "ϝ",
    ["f"] = "ϝ",
    ["G"] = "ɠ",
    ["g"] = "ɠ",
    ["H"] = "ꜧ",
    ["h"] = "ꜧ",
    ["I"] = "ı",
    ["i"] = "ı",
    ["J"] = "ʝ",
    ["j"] = "ʝ",
    ["K"] = "ƙ",
    ["k"] = "ƙ",
    ["L"] = "ʅ",
    ["l"] = "ʅ",
    ["M"] = "ɱ",
    ["m"] = "ɱ",
    ["N"] = "и",
    ["n"] = "и",
    ["O"] = "σ",
    ["o"] = "σ",
    ["P"] = "ρ",
    ["p"] = "ρ",
    ["Q"] = "ϙ",
    ["q"] = "ϙ",
    ["R"] = "ɾ",
    ["r"] = "ɾ",
    ["S"] = "ʂ",
    ["s"] = "ʂ",
    ["T"] = "ƚ",
    ["t"] = "ƚ",
    ["U"] = "υ",
    ["u"] = "υ",
    ["V"] = "ѵ",
    ["v"] = "ѵ",
    ["W"] = "ω",
    ["w"] = "ω",
    ["X"] = "ﾒ",
    ["x"] = "ﾒ",
    ["Y"] = "ყ",
    ["y"] = "ყ",
    ["Z"] = "ɀ",
    ["z"] = "ɀ",
    ["0"] = "０",
    ["1"] = "１",
    ["2"] = "２",
    ["3"] = "３",
    ["4"] = "４",
    ["5"] = "５",
    ["6"] = "６",
    ["7"] = "７",
    ["8"] = "８",
    ["9"] = "９",
    [" "] = " "
}

    local function filter(message)
        for search, replacement in pairs(letters) do
            message = replace(message, search, replacement)
        end
        return message
    end
    local function showNotification(title, text)
        StarterGui:SetCore("SendNotification", {Title = title, Text = text, Duration = 5})
    end
    local function sendChat(msg)
    local filteredMessage = msg
    local tagged = filteredMessage ~= msg
    local keepTextCheckbox = script.Parent.Parent.Parent:FindFirstChild("KeepTextCheckbox")
    local keepTextTick = keepTextCheckbox and keepTextCheckbox:FindFirstChild("Tick")

    if tagged then
        showNotification("HELL NAH !!", "TEXT FUCKED")
        textBox.Text = ""
    else
        if TextChatService.ChatVersion == Enum.ChatVersion.LegacyChatService then
            ReplicatedStorage:FindFirstChild("DefaultChatSystemChatEvents").SayMessageRequest:FireServer(msg, "All")
        else
            if chat then
            chat:SendAsync(msg)
        else
            warn("Chat channel not found")
        end
        end
        
        -- ONLY clear if checkbox is NOT checked
        if keepTextTick and keepTextTick.TextTransparency == 1 then
            textBox.Text = ""
        end
    end
end

    local function processText()
        local inputText = textBox.Text
        local convertedText = filter(inputText)
        sendChat(convertedText)
    end
    textButton.MouseButton1Click:Connect(processText)
    textBox.FocusLost:Connect(
        function(enterPressed)
            if enterPressed then
                processText()
            end
        end
    )
end
task.spawn(C_a)
local function C_11()
    local script = G2L["11"]
    local main = script.Parent.Parent.ImageLabel
    local button = script.Parent
    local function toggleVisibility()
        for _, child in ipairs(main:GetChildren()) do
            if child:IsA("TextBox") or child:IsA("TextButton") or child:IsA("TextLabel") then
                child.Visible = not child.Visible
            end
        end
    end
    button.MouseButton1Click:Connect(toggleVisibility)
end
task.spawn(C_11)
local function C_14()
    local script = G2L["14"]
    local main = script.Parent.Parent -- The Frame
    local button = script.Parent
    local minimized = false
    local tweenService = game:GetService("TweenService")

    -- Find the special ImageLabel
    local specialImage
    for _, child in ipairs(main:GetDescendants()) do
        if child:IsA("ImageLabel") and child.Image == "rbxassetid://106628123920796" then
            specialImage = child
            break
        end
    end

    -- Gather all UI elements to hide (except button and frame)
    local contentToToggle = {}
    for _, child in ipairs(main:GetDescendants()) do
        if (child:IsA("TextBox") or child:IsA("TextButton") or child:IsA("TextLabel")) and child ~= button then
            table.insert(contentToToggle, child)
        end
    end

    -- Save original background color
    local originalColor = main.BackgroundColor3

    button.MouseButton1Click:Connect(function()
        minimized = not minimized

        if minimized then
            -- Hide content
            for _, ui in ipairs(contentToToggle) do
                ui.Visible = false
            end
            if specialImage then
                specialImage.Visible = false
            end

            -- Animate frame smaller and change color
            local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
            local tween = tweenService:Create(main, tweenInfo, {
                Size = UDim2.new(0, 120, 0, 30),
                BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            })
            tween:Play()

        else
            -- Animate back to full size and change color
            local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
            local tween = tweenService:Create(main, tweenInfo, {
                Size = UDim2.new(0, 316, 0, 128),
                BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            })
            tween:Play()

            tween.Completed:Connect(function()
                -- Restore content after animation
                for _, ui in ipairs(contentToToggle) do
                    ui.Visible = true
                end
                if specialImage then
                    specialImage.Visible = true
                end
            end)
        end
    end)
end
task.spawn(C_14)
local function C_18()
    local script = G2L["18"]
    local button = script.Parent
    local tick = button:WaitForChild("Tick")
    local keepText = false -- Start UNCHECKED

    local TweenService = game:GetService("TweenService")
    local tweenInfo = TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)

    local function updateTick()
        local tween = TweenService:Create(tick, tweenInfo, {TextTransparency = keepText and 0 or 1})
        tween:Play()
    end

    updateTick()

    button.MouseButton1Click:Connect(function()
        keepText = not keepText
        updateTick()
    end)

    local sendButtonScript = G2L["a"] -- assuming your send button is ["a"]
    sendButtonScript.Parent.MouseButton1Click:Connect(function()
        local textBox = sendButtonScript.Parent.Parent.TextBox
        local inputText = textBox.Text
        local convertedText = sendButtonScript.Parent.Parent.TextBox.Parent.Parent.filter(inputText)
        sendButtonScript.Parent.Parent.TextBox.Parent.Parent.sendChat(convertedText)

        -- Only clear text if "keepText" is FALSE (unchecked)
        if not keepText then
            textBox.Text = ""
        end
    end)
end
